\name{GenerateLmPlot}
\alias{GenerateLmPlot}
\title{Generate Regression and Residual Plot}
\description{Generate a regression and residual plot.}
\usage{
GenerateLmPlot(x, y, xlim, ylim1, ylim2.mult)
}
\arguments{
  \item{x}{Variable for the x-axis.}
  \item{y}{Variable for the y-axis.}
  \item{xlim}{Limits for x-axis.}
  \item{ylim1}{Limits for y-axis.}
  \item{ylim2.mult}{Factor to inflate the y-limits for the residual plot.}
}
\author{David Diez (david@openintro.org)}
\seealso{
\code{\link{lmPlot}}
}
